<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Resumizer Pro</title>
    <!-- CUSTOM CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/uikit/2.26.3/css/uikit.min.css" rel="stylesheet" type="text/css">
    <!-- Bootstrap Core CSS -->

    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>

    <!-- Theme CSS -->
    <link href="css/agency.min.css" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body id="page-top" ng-app = "myApp" class="index">

    <!-- Navigation -->
    <nav id="mainNav" class="navbar navbar-default navbar-custom navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span> Menu <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand page-scroll" href="#page-top">Resumizer Pro</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="hidden">
                        <a href="#page-top"></a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#create">Create a Resume</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#samples">Browse Sample Resumes</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#contact">Contact</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="/about">About Us</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>

    <!-- Header -->
    <header>
      <div class = "row" ng-controller = "FormCtrl">
        <div class = "col-xs-4" style="margin-top: 10%">
          <form class="form-horizontal login" action="/resume" style="background: #FAFAFA; padding: 10px; border-radius:10%" method="post">
              <ul class = "pagination pagination-sm" style="font-family: Sans-serif">
                <li ng-class="{active: isActive(currentPage, page)}"ng-repeat="page in Pages"><a style="text-decoration: none" ng-click="setCurrent(page)" class="btn btn-block">{{page|NoUnderScore}}</a></li>
              </ul>
            <div class="form-group" ng-show="isActive(currentPage, Pages[0])" ng-repeat="contact in ContactInfo">
                <label for="inputEmail3" style="color: black" class="col-sm-2 control-label">{{contact|NoUnderScore| titlesize}}</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control loginform" name='contact_info[{{contact}}]' id={{contact}} placeholder={{contact|NoUnderScore|titlesize}}>
                </div>
            </div>
            <div class="form-group" ng-show="isActive(currentPage, Pages[1])" ng-repeat="contact in Education">
                <label for="inputEmail3" style="color: black" class="col-sm-2 control-label">{{contact|NoUnderScore| titlesize}}</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control loginform" name='education[{{contact}}]' id={{contact}} placeholder={{contact|NoUnderScore|titlesize}}>
                </div>
            </div>
            <div class="form-group" ng-show="isActive(currentPage, Pages[2])" ng-repeat="contact in Skills">
                <label for="inputEmail3" style="color: black" class="col-sm-2 control-label">{{contact|NoUnderScore| titlesize}}</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control loginform" name='Skills[{{contact}}]' id={{contact}} placeholder={{contact|NoUnderScore|titlesize}}>
                </div>
            </div>
            <div class="form-group" ng-show="isActive(currentPage, Pages[3])" ng-repeat="contact in Projects">
                <label for="inputEmail3" style="color: black" class="col-sm-2 control-label">{{contact|NoUnderScore| titlesize}}</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control loginform" name='Projects[{{contact}}]' id={{contact}} placeholder={{contact|NoUnderScore|titlesize}}>
                    <!--<a style="text-decoration: none" ng-click="addMore(contact)" class="btn btn-block">{{page|NoUnderScore}}</a> -->
                </div>
            </div>
            <div class="form-group" ng-show="isActive(currentPage, Pages[4])" ng-repeat="contact in WorkExperience">
                <label for="inputEmail3" style="color: black" class="col-sm-2 control-label">{{contact|NoUnderScore| titlesize}}</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control loginform" name='work_experence[{{contact}}]' id={{contact}} placeholder={{contact|NoUnderScore}}>
                </div>
            </div>

              <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                  <label>Do you want this resume being seen by recruiters?:
                    <input type="checkbox" name = "isPublic" ng-model="checkboxModel.value1">
                  </label>
                  <label>Do you want to be contacted by recruiters?:
                    <input type="checkbox" name = "canContact" ng-model="checkboxModel.value1">
                  </label>
                </div>
              </div>
              <div class="form-group">
                  <div class="col-sm-offset-2 col-sm-10">
                      <button type="submit" class="btn btn-default">Save</button>
                      <a href="/somewhereelse" class="btn btn-default">Done</a>
                  </div>
              </div>
          </form>






        </div>
        <div class = "col-xs-8" style="margin-top: 10%">
          <iframe id="fred" style="border:1px solid #666CCC" title="PDF in an i-Frame" src="/template" frameborder="1" scrolling="auto" width="100%" height="900" max-width="850" ></iframe>
        </div>
      </div>
    </header>



    <!-- jQuery -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/angular/angular.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="app/app.js"></script>
    <script type="text/javascript" src="app/controllers.js"></script>
    <script type="text/javascript" src="app/services.js"></script>
    <script type="text/javascript" src="app/filters.js"></script>
    <!-- Plugin JavaScript -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>

    <!-- Contact Form JavaScript -->
    <script src="js/jqBootstrapValidation.js"></script>
    <script src="js/contact_me.js"></script>

    <!-- CUSTOM JAVASCRIPT -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/2.26.3/js/uikit.min.js"></script>

    <!-- Theme JavaScript -->
    <script src="js/agency.min.js"></script>

</body>

</html>

